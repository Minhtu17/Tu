#include <stdio.h>

int main(void) {
    int count = 0;
    printf("Cac so nguyen co 2 chu so va la boi cua 7:\n");
    for (int i = 10; i < 100; i++) {
        if (i % 7 == 0) {
            printf("%d ", i);
            count++;
            if (count == 10) // Giới hạn số lượng số được xuất ra
                break;
        }
    }
    return 0;
}

